@model List<BlogDataModel>
<table class="table table-borered" id="tbDataTable">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Author</th>
            <th>Content</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Blog_Id
                </td>
                <td>
                    @item.Blog_Title
                </td>
                <td>
                    @item.Blog_Author
                </td>
                <td>
                    @item.Blog_Content
                </td>
            </tr>
        }
    </tbody>
</table>